                    <th>Id</th>
                    <th>Nombre</th>
                    <th>Descripcion</th>
                    <th>Categoria</th>
                </tr>
            </thead>
            <tbody>
                <tr v-for="producto in listaDeProductos">
                    <td>{{producto.Id}}</td>
                    <td>{{producto.Name}}</td>
                    <td>{{producto.Description}}</td>
                    <td>{{producto.CategoryName}}</td>
